import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        long a = sc.nextLong();      
        String op = sc.next();     
        long b = sc.nextLong();      
        String eq = sc.next();     
        long c = sc.nextLong();      

        long result = 0;

        if (op.equals("+")) {
            result = a + b;
        } else if (op.equals("-")) {
            result = a - b;
        } else if (op.equals("*")) {
            result = a * b;
        }

        if (result == c) {
            System.out.println("Yes");
        } else {
            System.out.println(result);
        }

        sc.close();
    }
}
