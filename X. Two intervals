import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long a = sc.nextLong();
        long b = sc.nextLong();
        long c = sc.nextLong();
        long d = sc.nextLong();

       long start = Math.max(a, c);
       
       long end = Math.min(b, d);

       if (start <= end) {
           System.out.println(start + " " + end);
       } else {
           System.out.println("-1");
       }
    }
}
